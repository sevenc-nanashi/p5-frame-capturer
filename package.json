{
  "name": "p5-frame-capturer",
  "version": "0.4.2",
  "description": "Capture p5.js frames and saves into your local file system",
  "packageManager": "pnpm@10.29.2",
  "author": {
    "name": "Nanashi.",
    "url": "https://sevenc7c.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sevenc-nanashi/p5-frame-capturer.git"
  },
  "homepage": "https://github.com/sevenc-nanashi/p5-frame-capturer",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "biome check",
    "lint:fix": "biome check --write --unsafe",
    "clean": "rm -rf dist",
    "build": "run-s clean build:bundle",
    "build:bundle": "cross-env NODE_ENV=production tsdown",
    "dev": "cross-env NODE_ENV=development tsdown --watch",
    "webp:build": "node --experimental-strip-types ./webp/build.ts",
    "webp:notice": "cd webp && cargo about generate about.hbs > ../NOTICE.md",
    "prepublishOnly": "run-s webp:build webp:notice build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [],
  "files": [
    "dist",
    "./NOTICE.md",
    "./README.md",
    "./LICENSE"
  ],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^25.2.2",
    "@types/wicg-file-system-access": "^2023.10.7",
    "cross-env": "^10.1.0",
    "csstype": "^3.2.3",
    "npm-run-all2": "^8.0.4",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "zx": "^8.8.5"
  },
  "dependencies": {
    "vanjs-core": "^1.6.0"
  },
  "optionalDependencies": {
    "p5": "^2.2.0"
  }
}
